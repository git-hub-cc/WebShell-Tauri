{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "webchat",
  "version": "0.1.0",
  "identifier": "club.ppmc.claims",

  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "",
    "beforeDevCommand": ""
  },

  "app": {
    "windows": [
      {
        "title": "webchat",
        "fullscreen": false,
        "width": 1650,
        "height": 950,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true
      }
    }
  },

  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}